    "import numpy as np\n",
    "\"Vacisek Model ZCB\"\n",
    "def exact_zcb(theta, kappa, sigma, tau, r0=0.):\n",
    "    B = (1 - np.exp(-kappa*tau)) / kappa\n",
    "    A = np.exp((theta-(sigma**2)/(2*(kappa**2))) *\n",
    "              (B-tau) - (sigma**2)/(4*kappa)*(B**2))\n",
    "    return A * np.exp(-r0*B)"
   ]
